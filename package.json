{
  "name": "truora",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsnd --respawn src/index.ts",
    "start": "npm run build && node dist/index.js",
    "build": "tsc --project tsconfig.prod.json",
    "test": "jest",
    "test:e2e": "jest --testMatch='**/?(*.)+(e2e-test).[tj]s?(x)'",
    "typecheck": "tsc",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "format": "npx prettier --check .",
    "format:fix": "npx prettier --write .",
    "prepare": "husky",
    "db:populate": "tsnd scripts/populate-db.ts",
    "db:generate": "prisma generate --schema=./src/modules/shared/infraestructure/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./src/modules/shared/infraestructure/prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=./src/modules/shared/infraestructure/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./src/modules/shared/infraestructure/prisma/schema.prisma",
    "db:pull": "prisma db pull --schema=./src/modules/shared/infraestructure/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "6.12.0",
    "axios": "1.10.0",
    "cors": "2.8.5",
    "express": "5.1.0",
    "express-promise-router": "4.1.1",
    "helmet": "8.1.0",
    "http-status": "2.1.0",
    "ulidx": "2.4.1",
    "yahoo-finance2": "^2.13.3",
    "zod": "4.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.31.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.1.0",
    "@types/supertest": "6.0.3",
    "commitlint": "19.8.1",
    "dotenv": "17.2.0",
    "eslint": "9.31.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "prettier": "3.6.2",
    "prisma": "6.12.0",
    "supertest": "7.1.4",
    "ts-jest": "29.4.0",
    "ts-node-dev": "2.0.0",
    "typescript-eslint": "8.38.0"
  }
}
